FROM nginx:1.12

COPY ./nginx /etc/nginx/snippets
COPY ./nginx/openssl /etc/letsencrypt/live
COPY ./nginx/conf /etc/conf
COPY ./robot /home/smsp/robot

COPY ./nginx/nginx-app.conf /etc/nginx/conf.d/default.conf
